# Program 9 : Program to count the number of digit of given number
# Input : 1345
# Output: 4

def DigitCount(no):
	digit = 0
	while (no != 0):
		
		no = int(no / 10)
		digit = digit + 1
	
	return digit
def main():
	print("Enter the number:")
	value = int(input())
	iret = DigitCount(value)
	print("Number of digits are:",iret)
if __name__ =="__main__":
	main()
