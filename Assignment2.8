# Program 10 : Program to return the addtion of digits of given number
# Input : 123
# Output : 6


def AddDigit(no):
	sum = 0
	while (no != 0):
		sum = sum + int(no % 10)
		no = int(no / 10)
	return sum
def main():
	print("Enter the number:")
	value = int(input())
	iret = AddDigit(value)
	print("Addition of digits are:",iret)
if __name__ =="__main__":
	main()
